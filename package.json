    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-newer": "^1.3.0",
    "gulp-run": "^1.7.1",
    "gulp-tslint": "^7.1.0",
    "gulp-typescript": "^3.1.5",
    "jest": "^17.0.3",
    "jest-junit": "^1.0.4",
    "reflect-metadata": "^0.1.10",
    "source-map-loader": "^0.2.1",
    "tslint": "^4.3.1",
    "typemoq": "^1.0.2",
    "typescript": "^2.0.10",
    "vrsource-tslint-rules": "^4.0.1",
    "webpack": "^2.3.3"
